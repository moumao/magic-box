{
  "presets": [
    "es2015",
    "react",
    "stage-0",
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-runtime",
    "transform-decorators-legacy",
    ["transform-async-to-module-method", {
      "module": "bluebird",
      "method": "coroutine"
    }],
    ["import", {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ]
}
